{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ACCFlatLapArray",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "lap": {
        "type": "integer"
      },
      "time": {
        "type": "object",
        "properties": {
          "time": {
            "type": "integer",
            "description": "lap total in ms"
          },
          "formattedTime": {
            "type": "string"
          }
        },
        "required": [
          "time"
        ]
      },
      "splits": {
        "type": "array",
        "items": {
          "type": "integer"
        },
        "minItems": 3,
        "maxItems": 3
      },
      "formattedSplits": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "minItems": 3,
        "maxItems": 3
      },
      "type": {
        "type": "string",
        "enum": [
          "Regular",
          "Outlap",
          "Inlap"
        ]
      },
      "valid": {
        "type": "boolean"
      },
      "boxLap": {
        "type": "boolean"
      },
      "clock": {
        "type": "string"
      },
      "position": {
        "type": "number"
      },
      "trackGripStatus": {
        "type": "string"
      },
      "tyre": {
        "type": "object",
        "properties": {
          "pressure": {
            "type": "object",
            "properties": {
              "frontLeft": {
                "type": "number"
              },
              "frontRight": {
                "type": "number"
              },
              "rearLeft": {
                "type": "number"
              },
              "rearRight": {
                "type": "number"
              }
            }
          },
          "temperature": {
            "type": "object",
            "properties": {
              "frontLeft": {
                "type": "number"
              },
              "frontRight": {
                "type": "number"
              },
              "rearLeft": {
                "type": "number"
              },
              "rearRight": {
                "type": "number"
              }
            }
          },
          "compound": {
            "type": "string"
          },
          "currentTyreSet": {
            "type": "integer"
          }
        }
      },
      "environment": {
        "type": "object",
        "properties": {
          "airTemperature": {
            "type": "number"
          },
          "roadTemperature": {
            "type": "number"
          },
          "rainIntensity": {
            "type": "string"
          },
          "windSpeed": {
            "type": "number"
          },
          "windDirection": {
            "type": "number"
          },
          "rainIntensityIn10min": {
            "type": "string"
          },
          "rainIntensityIn30min": {
            "type": "string"
          }
        }
      },
      "carInfo": {
        "type": "object",
        "properties": {
          "damage": {
            "type": "object"
          },
          "fuel": {
            "type": "number"
          },
          "fuelRealUsed": {
            "type": "number"
          },
          "fuelXLap": {
            "type": "number"
          },
          "usedFuel": {
            "type": "number"
          },
          "fuelEstimatedLaps": {
            "type": "number"
          },
          "tc": {
            "type": "number"
          },
          "tcCut": {
            "type": "number"
          },
          "abs": {
            "type": "number"
          }
        }
      }
    },
    "required": [
      "lap",
      "time",
      "valid"
    ]
  }
}